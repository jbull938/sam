  <row>
    <panel>
      <title>Percent of unassigned High or Critical incidents</title>
      <single>
        <search>
          <query>| inputlookup incident_review_lookup
| search urgency IN (high critical)
| eventstats count as total
| where isnull(owner)
| stats count as "unassigned" by total
| eval percentage=(unassigned*100)/total
| table percentage</query>
